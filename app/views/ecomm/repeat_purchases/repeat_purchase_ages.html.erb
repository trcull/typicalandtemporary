
<script type="application/javascript">
age_at_repeat_order_histogram = function (div, rv) {
	var series1 = rv['num_customers'][1];
	var series2 = rv['pct_customers'][1];
	var series3 = rv['pct_cumulative'][1];
	draw_graph(div, [series1, series2, series3],["Number","Percent","Cumulative Percent"],["Number of Customers","Percent of Customers","Cumulative Percent of Customers"],"Age in days at first repurchase");
};
</script>

<%= render partial: "/common/num_pct_cumulative_graph", locals: {data_url: "/api/v1/rollups/#{@organization_id}/age_at_repeat_order_histogram", success_function_name: 'age_at_repeat_order_histogram'}%>
